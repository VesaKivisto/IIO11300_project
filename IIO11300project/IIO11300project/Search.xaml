<Window x:Class="IIO11300project.Search"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IIO11300project"
        mc:Ignorable="d"
        Title="Search" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBox x:Name="txtSummonerName" Width="250" Margin="0, 0, 10, 0"/>
            <ComboBox x:Name="cbRegions" Width="60" Margin="0, 0, 10, 0" />
            <Button x:Name="btnSearch" Content="Search" Click="btnSearch_Click" />
        </StackPanel>
    </Grid>
</Window>
